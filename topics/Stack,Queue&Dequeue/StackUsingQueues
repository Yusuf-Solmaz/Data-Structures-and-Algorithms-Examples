from queue import Queue


class StackUsingQueues():

    def __init__(self):
        self.queue = Queue()
        
    def isEmpty(self):
        return self.queue == []
    
    
    def push(self,value):
        self.queue.put(value)
        
        
    def pop (self):
        for value in self.queue:
            self.queue.queue
            self.queue.get()
        self.queue.get()
        
        
        
class Test():
    
    stack = StackUsingQueues()
    
    stack.push(1)    
    stack.push(2)  
    stack.push(3)  
    stack.push(4)  
            
    print(stack.isEmpty)